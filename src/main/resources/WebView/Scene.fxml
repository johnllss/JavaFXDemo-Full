<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.web.WebView?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/25" xmlns:fx="http://javafx.com/fxml/1" fx:controller="WebView.Controller">
   <children>
      <WebView fx:id="myWebView" layoutY="62.0" prefHeight="338.0" prefWidth="591.0" />
      <Button fx:id="myBtn" layoutX="89.0" layoutY="39.0" mnemonicParsing="false" text="Load" />
      <TextField fx:id="myTextField" layoutX="89.0" layoutY="14.0" onAction="#loadPage" prefHeight="25.0" prefWidth="436.0" />
      <Button layoutX="152.0" layoutY="39.0" mnemonicParsing="false" onAction="#refreshPage" text="Refresh" />
      <Button layoutX="222.0" layoutY="39.0" mnemonicParsing="false" onAction="#zoomIn" text="+" />
      <Button layoutX="271.0" layoutY="39.0" mnemonicParsing="false" onAction="#zoomOut" text="-" />
      <Button layoutX="323.0" layoutY="39.0" mnemonicParsing="false" onAction="#displayHistory" text="History" />
      <Button layoutX="397.0" layoutY="39.0" mnemonicParsing="false" onAction="#back" text="Back" />
      <Button layoutX="460.0" layoutY="39.0" mnemonicParsing="false" onAction="#forward" text="Forward" />
      <Button layoutX="534.0" layoutY="39.0" mnemonicParsing="false" onAction="#executeJS" text="Execute.JS" />
   </children>
</AnchorPane>
